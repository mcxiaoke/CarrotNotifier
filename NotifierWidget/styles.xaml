<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:properties="clr-namespace:NotifierWidget.Properties">
    <system:Double x:Key="TextFontSize">15</system:Double>
    <Color x:Key="TextNormalColor">#eeeeee</Color>
    <Color x:Key="TextHighlightColor">#ffff00</Color>
    <Color x:Key="TextErrorColor">#ff0000</Color>
    <Color x:Key="BackgroundColor">#80000000</Color>
    <FontFamily x:Key="TextFontFamily">NSimSun,SimSun,Microsoft YaHei</FontFamily>
    <SolidColorBrush x:Key="TextColorBrush" Color="{StaticResource TextNormalColor}" />
    <SolidColorBrush x:Key="TextHighlightColorBrush" Color="{StaticResource TextHighlightColor}" />
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="{StaticResource BackgroundColor}" />

</ResourceDictionary>